    <!-- our service -->
    <div class="container-fluid">
      <div class="container">
        <div class="text text-center">
          <h1>Our Service</h1>
          <h2 style="margin-bottom: 50px">Laundry Kiloan</h2>
        </div>
      </div>
    </div>
    <div class="container-fluid">
      <div class="container">
        <!-- Laundry Kiloan -->
        <div class="row">
          <div class="card" style="width: 10.9rem; border: none">
            <img src="/images/basah.png" class="card-img-top" alt="basah" />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Cuci Basah
              </p>
              <p class="card-text text-center">5000/kg</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img src="/images/kering.png" class="card-img-top" alt="kering" />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Cuci Kering
              </p>
              <p class="card-text text-center">6000/kg</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img
              src="/images/kering-lipat.png"
              class="card-img-top"
              alt="kering-lipat"
            />
            <div class="card-body">
              <p
                class="card-text"
                style="margin-bottom: 0px; width: max-content"
              >
                Cuci Kering Lipat
              </p>
              <p class="card-text text-center">7000/kg</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img
              src="/images/kering-setrika.png"
              class="card-img-top"
              alt="kering-setrika"
            />
            <div class="card-body">
              <p
                class="card-text"
                style="margin-bottom: 0px; width: max-content"
              >
                Cuci Kering Setrika
              </p>
              <p class="card-text text-center">8500/kg</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img src="/images/setrika.png" class="card-img-top" alt="setrika" />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Setrika
              </p>
              <p class="card-text text-center">7500/kg</p>
            </div>
          </div>
        </div>
        <!-- Laundry Kiloan -->
        <!-- Laundry Satuan -->
        <h2 class="text-center" style="margin-bottom: 50px">Laundry Satuan</h2>
        <div class="row">
          <div class="card" style="width: 10.9rem; border: none">
            <img
              src="/images/gorden-tipis.png"
              class="card-img-top"
              alt="gorden-tipis"
            />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Gorden Tipis
              </p>
              <p class="card-text text-center">7500/meter</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img
              src="/images/gorden-tebal.png"
              class="card-img-top"
              alt="gorden-tebal"
            />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Gorden Tebal
              </p>
              <p class="card-text text-center">12500/meter</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img src="/images/selimut.png" class="card-img-top" alt="selimut" />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Selimut
              </p>
              <p class="card-text text-center">20000</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img
              src="/images/bed-cover.svg"
              class="card-img-top"
              alt="bed-cover"
            />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Bed Cover
              </p>
              <p class="card-text text-center">30000</p>
            </div>
          </div>
          <div class="card offset-1" style="width: 10.9rem; border: none">
            <img src="/images/jas.svg" class="card-img-top" alt="jas" />
            <div class="card-body">
              <p class="card-text text-center" style="margin-bottom: 0px">
                Jas
              </p>
              <p class="card-text text-center">35000</p>
            </div>
          </div>
        </div>
        <!-- Laundry Satuan -->
      </div>
    </div>
    <!-- our service -->

    <!-- Track Section -->
    <div class="container-fluid">
      <div class="container">
        <div class="row">
          <div class="col-12 text-center">
            <h2>Track Pesanan Anda</h2>
            <p>Silahkan isi ID pesanan Anda</p>
          </div>
        </div>
      </div>
    </div>
    <div class="container-fluid">
      <div class="container">
        <div class="row">
          <div class="col-6 offset-3">
            <form class="d-flex" role="search">
              <input
                class="form-control me-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
                name="track"
              />
              <button class="btn btn-outline-success" type="submit">
                Search
              </button>
            </form>
          </div>
        </div>
      </div>
      <p><%= items %></p>
    </div>
    <!-- Track Section -->

